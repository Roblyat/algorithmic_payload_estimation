<launch>

    <!-- Set use_sim_time parameter to true -->
    <param name="use_sim_time" value="true" />

    <!-- Include the MoveIt demo Gazebo launch file -->
    <include file="$(find manipulator_moveit_config)/launch/demo_gazebo.launch" />

    <!-- Start hmi -->
    <node pkg="hmi" type="hmi" name="hmi_node" output="screen" />   

</launch>
